{
  "name": "@lai/core",
  "version": "0.1.0",
  "description": "Core AI engine for LAI and UDP - multi-provider AI with privacy controls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p core/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint core/src/**/*.ts",
    "lint:fix": "eslint core/src/**/*.ts --fix",
    "format": "prettier --write \"core/src/**/*.ts\"",
    "format:check": "prettier --check \"core/src/**/*.ts\"",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test"
  },
  "keywords": [
    "ai",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "ollama",
    "llm",
    "privacy"
  ],
  "author": "tbmobb813",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "typescript": "^5.9.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tbmobb813/lai-core"
  }
}